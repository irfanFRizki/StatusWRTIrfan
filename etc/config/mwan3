
config globals 'globals'
	option mmx_mask '0x3F00'

config rule 'https'
	option sticky '1'
	option dest_port '443'
	option proto 'tcp'
	option use_policy 'balancing'

config rule 'default_rule_v4'
	option dest_ip '0.0.0.0/0'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option use_policy 'balancing'

config rule 'default_rule_v6'
	option dest_ip '::/0'
	option family 'ipv6'
	option proto 'all'
	option sticky '0'
	option use_policy 'balancing'

config interface 'wan'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option failure_interval '5'
	option recovery_interval '5'
	option interval '5'
	option down '2'
	option up '3'
	option reliability '1'
	option track_method 'httping'
	option count '1'
	option timeout '2'
	list track_ip 'google.com'
	option httping_ssl '1'

config member 'mm_net'
	option interface 'wan'
	option metric '1'
	option weight '1'

config interface 'tethering'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '0.0.0.0'
	option track_method 'ping'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option failure_interval '5'
	option recovery_interval '5'
	option timeout '2'
	option interval '5'
	option down '2'
	option up '3'
	option reliability '1'
	option count '1'

config member 'tethering_net'
	option interface 'tethering'
	option weight '1'
	option metric '2'

config policy 'balancing'
	option last_resort 'unreachable'
	list use_member 'mm_net'
	list use_member 'tethering_net'
	list use_member 'HP_net'
	list use_member 'byu_net'

config interface 'HP1'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '0.0.0.0'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option recovery_interval '5'
	option down '5'
	option up '5'

config member 'HP_net'
	option interface 'HP1'
	option metric '1'
	option weight '1'

config interface 'wan2'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option reliability '1'
	option failure_interval '5'
	option recovery_interval '5'
	option timeout '5'
	option interval '5'
	option down '2'
	option up '3'
	option count '1'
	list track_ip '0.0.0.0'
	option track_method 'ping'
	option size '56'
	option max_ttl '60'
	option check_quality '0'

config member 'byu_net'
	option interface 'wan2'
	option metric '2'
	option weight '1'

